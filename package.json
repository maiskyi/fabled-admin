    "@keystone-6/fields-document": "^9.0.0",
    "@keystone-ui/core": "^5.0.2",
    "@sentry/node": "^8.34.0",
    "@sentry/profiling-node": "^8.34.0",
    "firebase-admin": "^12.7.0",
    "got": "11.8.6",
    "graphql": "^16.9.0",
    "lodash": "^4.17.21",
    "micromustache": "^8.0.3",
    "openai": "^4.57.1",
    "type-fest": "^4.26.1",
    "winston": "^3.15.0",
    "winston-transport-sentry-node": "^3.0.0"
